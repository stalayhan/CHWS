main(void)
{
	double salary,
			normalPayment, overtimePayment,
			salaryPerHour,
			experiencePoint,
			workingYears,
			numberOfFinishedProjects,
			opb,	/* Overtime Payment Base */
			weo,	/* Weekend Overtime */
			wdo;	/* Weekday Overtime */

			
			salaryPerHour = BHP +
			
			normalPayment = salaryPerHour * WORKING_HOURS;
			/* Calculate salary simple formul. */
			salary = normalPayment + overtimePayment;

	return 0;
}